<% renderMenu = (items) => %>
<ul class="nav nav-pills nav-stacked">
    <% for item in items: %>        
        <li<% if item.state and item.state == 'current' : %> class="active"<% end %>>
            <a href="<%= item.url %>"><%= item.title %></a>
            <% if item.children: %>
                <%- renderMenu(item.children) %>
            <% end %>
        </li>
    <% end %>
</ul>   
<% end %>

<%= renderMenu @menuItems %>